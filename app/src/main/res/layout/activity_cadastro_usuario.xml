<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadastroUsuario">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:text="Área"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="394dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data de nascimento"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="394dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="45dp"
        android:layout_height="18dp"
        android:text="Sexo"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="505dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor diária/hora"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="504dp" />

    <Button
        android:id="@+id/btnCadastroUsuario"
        android:layout_width="298dp"
        android:layout_height="43dp"
        android:text="Cadastrar"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="641dp" />

    <EditText
        android:id="@+id/cpfUsuario"
        android:layout_width="358dp"
        android:layout_height="55dp"
        android:ems="10"
        android:hint="CPF"
        android:inputType="number"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="229dp" />

    <EditText
        android:id="@+id/nomeUsuario"
        android:layout_width="358dp"
        android:layout_height="48dp"
        android:ems="10"
        android:hint="Nome Completo"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="315dp" />

    <EditText
        android:id="@+id/nascimentoUsuario"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="430dp" />

    <EditText
        android:id="@+id/valorUsuario"
        android:layout_width="137dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="544dp" />

    <Spinner
        android:id="@+id/sexoUsuario"
        android:layout_width="170dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="546dp" />

    <Spinner
        android:id="@+id/areaUsuario"
        android:layout_width="171dp"
        android:layout_height="43dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="432dp" />

    <EditText
        android:id="@+id/emailUsuario"
        android:layout_width="362dp"
        android:layout_height="47dp"
        android:ems="10"
        android:hint="E-mail"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="59dp" />

    <EditText
        android:id="@+id/senhaUsuario"
        android:layout_width="363dp"
        android:layout_height="58dp"
        android:ems="10"
        android:hint="Senha"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="139dp" />

</android.support.constraint.ConstraintLayout>