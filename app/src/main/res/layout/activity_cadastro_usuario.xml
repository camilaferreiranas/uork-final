<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CadastroUsuario">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CPF"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="238dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome completo"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="349dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:text="Área"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="456dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data de nascimento"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="456dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sexo"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="567dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor hora"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="567dp" />

    <Button
        android:id="@+id/btnCadastroUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="667dp" />

    <EditText
        android:id="@+id/cpfUsuario"
        android:layout_width="350dp"
        android:layout_height="53dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="276dp" />

    <EditText
        android:id="@+id/nomeUsuario"
        android:layout_width="352dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="388dp" />

    <EditText
        android:id="@+id/nascimentoUsuario"
        android:layout_width="138dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="495dp" />

    <EditText
        android:id="@+id/valorUsuario"
        android:layout_width="137dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="604dp" />

    <Spinner
        android:id="@+id/sexoUsuario"
        android:layout_width="170dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="608dp" />

    <Spinner
        android:id="@+id/areaUsuario"
        android:layout_width="171dp"
        android:layout_height="43dp"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="497dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-mail"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/emailUsuario"
        android:layout_width="362dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="59dp" />

    <EditText
        android:id="@+id/senhaUsuario"
        android:layout_width="363dp"
        android:layout_height="58dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senha"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="120dp" />

</android.support.constraint.ConstraintLayout>