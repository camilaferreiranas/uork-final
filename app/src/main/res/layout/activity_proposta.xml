<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Proposta">

    <TextView
        android:id="@+id/tituloProposta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="203dp"
        tools:layout_editor_absoluteY="80dp" />

    <Button
        android:id="@+id/btnEnviarProposta"
        android:layout_width="364dp"
        android:layout_height="78dp"
        android:text="Enviar proposta"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="515dp" />

    <ImageView
        android:id="@+id/fotoServico"
        android:layout_width="128dp"
        android:layout_height="106dp"
        app:srcCompat="@mipmap/ic_launcher_round"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/descricaoServico"
        android:layout_width="370dp"
        android:layout_height="70dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="186dp" />

    <TextView
        android:id="@+id/orcamentoProposta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="304dp" />

    <TextView
        android:id="@+id/localServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="304dp" />
</android.support.constraint.ConstraintLayout>